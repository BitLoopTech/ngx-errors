{
  "name": "ngx-errors-complete",
  "version": "4.3.0",
  "description": "A declarative validation module for reactive forms",
  "author": "Ultimate Angular",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/BitLoopTech/ngx-errors.git",
    "type": "git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run copy && yarn run compile",
    "clean": "rimraf dist && mkdir dist",
    "compile": "yarn run ngc && yarn run rollup",
    "copy": "del /q dist\\package.json && copy package.json dist\\ && del /q dist\\README.md && copy README.md dist\\",
    "example": "webpack-dev-server --inline --hot",
    "prestart": "yarn run clean && yarn run copy",
    "start": "nodemon --exec yarn run compile -e ts --watch src",
    "rollup": "rollup -c rollup.config.js > NUL 2>&1",
    "ngc": "ngc -p tsconfig.json",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "prepublish": "yarn run test && yarn run build"
  },
  "devDependencies": {
    "@angular/compiler": "9.1.3",
    "@angular/compiler-cli": "9.1.3",
    "@types/jasmine": "2.5.46",
    "@types/karma": "5.0.0",
    "@types/node": "^13.13.4",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "jasmine-core": "2.5.2",
    "karma": "5.0.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.28",
    "karma-webpack": "4.0.2",
    "node-sass": "4.13.1",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "rollup": "0.41.6",
    "rxjs": "6.5.5",
    "rxjs-tslint": "^0.1.8",
    "sass-loader": "6.0.3",
    "tslint": "^5.0.0",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "zone.js": "0.10.3"
  },
  "dependencies": {
    "@angular/animations": "9.1.3",
    "@angular/cli": "9.1.3",
    "@angular/common": "9.1.3",
    "@angular/core": "9.1.3",
    "@angular/forms": "9.1.3",
    "@angular/platform-browser": "9.1.3",
    "@angular/platform-browser-dynamic": "9.1.3",
    "@angular/router": "9.1.3",
    "core-js": "3.6.5",
    "reflect-metadata": "0.1.10",
    "tslib": "1.11.1"
  }
}
